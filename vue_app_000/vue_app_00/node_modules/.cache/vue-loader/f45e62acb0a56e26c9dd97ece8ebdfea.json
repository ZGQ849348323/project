ts_normal.png')\"\r\n\t\t\t\t\t>\t\r\n\t\t\t\t</TabBarlcon>\r\n\t\t\t\t我\r\n\t\t\t</mt-tab-item>\r\n\t\t</mt-tabbar>\r\n\t</div>\r\n</template>\r\n<script>\r\n// 1：引入TitleBar.vue\r\nimport TitleBar from \"./common/TitleBar.vue\"\r\nimport MessageList from \"./common/MessageList\"\r\nimport TabBarlcon from \"./common/TabBarlcon\"\r\nexport default {\r\n\tdata(){\r\n\t\treturn{\r\n\t\t\tactive:\"message\",\r\n\t\t\tcurrentIndex:[\r\n\t\t\t\t{isSelect:true},    //0保存第一个按钮的状态\r\n\t\t\t\t{isSelect:false},\t//1保存第二个按钮的状态\r\n\t\t\t\t{isSelect:false},\t//2保存第三个按钮的状态\r\n\t\t\t\t{isSelect:false},\t//3保存第是个按钮的状态\r\n\t\t\t]\r\n\t\t}\r\n\t},\r\n\tmethods: {\r\n\t\tmysearch(){\r\n\t\t\t//titleBar子组件所需的处理函数\r\n\t\t\tconsole.log(\"搜索\")\r\n\t\t},\r\n\t\tmyadd3(){\r\n\t\t\tconsole.log(\"添加\")\r\n\t\t},\r\n\t\tchangeState(idx){\r\n\t\t\t//指定当前的按钮状态修改为true\r\n\t\t\t//  其他的按钮状态修改  false\r\n\t\t\t// 1:创建循环遍历状态组数\r\n\t\t\tfor(var i=0; i<this.currentIndex.length;i++){\r\n\t\t\t// 2:获取用户点击下标\r\n\t\t\tvar ui=idx;\r\n\t\t\t\t// 与当前数据元素下标比较\r\n\t\t\t\tif(ui==i){\r\n\t\t\t// 3:如果两个值相等当前状态true\r\n\t\t\t\t\tthis.currentIndex[i].isSelect=true;\r\n\t\t\t// 4：其他元素状态false\r\n\t\t\t\t}else{\r\n\t\t\t\t\tthis.currentIndex[i].isSelect=false;\r\n\t\t\t\t}\r\n\t\t\t}\r\n\t\t}\t\t\r\n\t},\r\n\tcomponents:{\r\n\t\t\"TitleBar\":TitleBar,\r\n\t\t\"MessageList\":MessageList,\r\n\t\t\"TabBarlcon\":TabBarlcon,\r\n\t}\r\n}\r\n</script>\r\n<style scoped>\r\n\t/* 3：修改组件默认样式 */\r\n\t.mint-tabbar>.mint-tab-item{\r\n\t\tcolor:#999;\r\n\t}\r\n\t/* 4：修改组件选中样式 */\r\n\t.mint-tabbar>.mint-tab-item.is-selected{\r\n\t\tbackground-color: transparent;\r\n\t\tcolor: #45c018;  /*选中绿色*/\r\n\t}\r\n</style>"]}]}